<% items.each do |item| %>
	<div>
        <%= link_to end_users_item_path(item.id) do %>
            <%= attachment_image_tag(item, :image, :fill, 100, 100, fallback: "noimage.png",size: "100x100") %>
        <% end %>
        <%= link_to item.item_name, end_users_item_path(item.id) %>
    </div>
<% end %>